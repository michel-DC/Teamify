{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "git": {
    "deploymentEnabled": {
      "nonâ€‘stable": false,
      "non-stable-google-auth": false,
      "non-stable-message": false,
      "stable": false,
      "main": true
    }
  },
  "buildCommand": "prisma generate && next build",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "src/app/api/**/*.ts": {
      "maxDuration": 30
    }
  },
  "regions": ["iad1"],
  "build": {
    "env": {
      "VERCEL_LOG_LEVEL": "info"
    }
  },
  "rewrites": [
    {
      "source": "/socket.io/(.*)",
      "destination": "https://teamify-pre-production.up.railway.app/socket.io/$1"
    }
  ],
  "headers": [
    {
      "source": "/api/images/signed-url",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, PUT, DELETE, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ]
}
