# Variables d'environnement pour les tests Cypress
# Copiez ce fichier vers .env.test et configurez vos valeurs

# Base de données de test
DATABASE_URL="postgresql://username:password@localhost:5432/teamify_test"

# NextAuth pour les tests
NEXTAUTH_URL="http://localhost:3000"
NEXTAUTH_SECRET="test-secret-key"

# API Keys pour les tests (utiliser des clés de test)
RESEND_API_KEY="re_test_key"
PUSHER_APP_ID="test_app_id"
PUSHER_KEY="test_key"
PUSHER_SECRET="test_secret"
PUSHER_CLUSTER="eu"

# URLs
NEXT_PUBLIC_APP_URL="http://localhost:3000"
NEXT_PUBLIC_PUSHER_KEY="test_key"
NEXT_PUBLIC_PUSHER_CLUSTER="eu"

# Google OAuth pour les tests
GOOGLE_CLIENT_ID="test_google_client_id"
GOOGLE_CLIENT_SECRET="test_google_client_secret"

# AWS S3 pour les tests (utiliser un bucket de test)
AWS_ACCESS_KEY_ID="test_access_key"
AWS_SECRET_ACCESS_KEY="test_secret_key"
AWS_REGION="eu-west-1"
AWS_S3_BUCKET="teamify-test-bucket"
